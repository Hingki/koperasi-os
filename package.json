  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.47.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "globals": "^16.5.0",
    "pgsql-ast-parser": "^1.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.50.0",
    "wait-on": "^7.0.1"
  }
